\begin{tikzpicture}
\draw[ultra thick, <->] (-4,0)--(4,0);
\YExcoord{0}{3}
\YExcoord{-3}{\textcolor{red}{b}}
\YExcoord{3}{\textcolor{blue}{a}}
\draw[decorate, decoration={brace, amplitude=10pt, mirror}, red] (-3,-.75)--(0,-.75);
